{
	"name": "flux-lens-extension",
	"version": "0.0.3",
	"description": "Add flux commands to lens",
	"publisher": "@KenWestgate/ken-westgate",
	"engines": {
		"lens": "^6.4.13"
	},
	"main": "dist/main.js",
	"renderer": "dist/renderer.js",
	"contributes": {
		"crds": [],
		"cloudProviders": [],
		"kubernetesDistros": []
	},
	"keywords": [
		"flux",
		"lens",
		"extension",
		"k8slens",
		"kubernetes"
	],
	"scripts": {
